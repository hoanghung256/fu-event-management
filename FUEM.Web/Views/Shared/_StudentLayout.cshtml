<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FUEM.Web</title>
    <link rel="stylesheet" href="~/theme/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/meanmenu.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/animate.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/metisMenu.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/swiper-bundle.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/slick.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/backtotop.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/magnific-popup.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/flaticon_expovent.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/fontawesome-pro.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/spacing.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/theme/css/main.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">FUEM.Web</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - FUEM.Web
        </div>
    </footer>
    @* <script src="~/lib/jquery/dist/jquery.min.js"></script> *@
    <script src="~/theme/js/bootstrap.bundle.min.js"></script>
    <script src="~/theme/js/jquery-3.6.0.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/waypoints.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/apexcharts.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/metisMenu.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/meanmenu.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/swiper-bundle.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/slick.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/magnific-popup.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/backtotop.js" asp-append-version="true"></script>
    <script src="~/theme/js/counterup.js" asp-append-version="true"></script>
    <script src="~/theme/js/wow.min.js" asp-append-version="true"></script>
    <script src="~/theme/js/countdown.js" asp-append-version="true"></script>
    <script src="~/theme/js/smooth-scrollbar.js" asp-append-version="true"></script>
    <script src="~/theme/js/ajax-form.js" asp-append-version="true"></script>
    <script src="~/theme/js/custom.js" asp-append-version="true"></script>
    <script src="~/theme/js/main.js" asp-append-version="true"></script>
    @await Html.PartialAsync("_ToastSetUp")
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
